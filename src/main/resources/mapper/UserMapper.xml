<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ddrdaccountingmanagement.repository.UserRepository">
    <select id="selectUserList" resultType="com.ddrdaccountingmanagement.domain.UserDTO">
        select
        user_no
        , user_nm
        , user_id
        , password
        , user_info
        , user_rank
        , reg_no
        , reg_dt
        , upd_no
        , upd_dt
        from
            com_user
    </select>
</mapper>